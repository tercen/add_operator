{
  "name": "Add",
  "description": "Calculates the sum of two values.",
  "tags": [
    "arithmetic"
  ],
  "authors": [
    "tercen"
  ],
  "urls": [
    "https://github.com/tercen/add_operator"
  ],
  "container": "ghcr.io/tercen/add_operator:master",
  "properties": [],
  "operatorSpec": {
    "kind": "OperatorSpec",
    "ontologyUri": "https://tercen.com/_ontology/tercen",
    "ontologyVersion": "0.0.1",
    "inputSpecs": [
      {
        "kind": "CrosstabSpec",
        "metaFactors": [],
        "axis": [
          {
            "kind": "AxisSpec",
            "metaFactors": [
              {
                "kind": "MetaFactor",
                "name": "Initial value",
                "type": "numeric",
                "description": "Initial value before addition",
                "ontologyMapping": "measurement",
                "crosstabMapping": "y",
                "cardinality": "1",
                "factors": []
              },
              {
                "kind": "MetaFactor",
                "name": "Added value",
                "type": "numeric",
                "description": "Value to be added to the initial value",
                "ontologyMapping": "measurement",
                "crosstabMapping": "x",
                "cardinality": "1",
                "factors": []
              }
            ]
          }
        ]
      }
    ],
    "outputSpecsV2": [
      {
        "kind": "OperatorJoinSpec",
        "joinOperators": [
          {
            "kind": "JoinOperator",
            "joinType": "left",
            "leftPair": {
              "kind": "ColumnPair",
              "lColumns": [],
              "rColumns": []
            },
            "rightRelation": {
              "kind": "TableRelation",
              "attributes": [
                {
                  "kind": "Attribute",
                  "name": "sum",
                  "type": "double"
                }
              ]
            }
          }
        ]
      }
    ]
  }
}